{
  "compilerOptions": {
    "baseUrl": "../",
    "forceConsistentCasingInFileNames": true,
    "lib": ["es6"],
    "module": "commonjs",
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "paths": { "mylib": ["."] },
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "types": [],
    "typeRoots": ["../"]
  }
}
